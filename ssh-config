# default IdentityFile
IdentityFile ~/.ssh/github-ogios

Host github.com
  Hostname ssh.github.com
  Port 443

Host github.com-ogios
  Hostname ssh.github.com
  User git
  Port 443
  IdentityFile ~/.ssh/github-ogios

Host github.com-sussybot5258
  Hostname ssh.github.com
  User git
  Port 443
  IdentityFile ~/.ssh/sussybot5258

Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur
